{
  "name": "@nullvoxpopuli/eslint-configs",
  "version": "4.3.0",
  "description": "eslint configs for the NullVoxPopuli's projects",
  "repository": {
    "url": "https://github.com/NullVoxPopuli/eslint-configs"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "type": "module",
  "imports": {
    "#utils": "./src/utils.js",
    "#types": "./src/types.ts"
  },
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:all": "pnpm --filter '*' format",
    "format:check:all": "pnpm --filter '*-prettier' format:check",
    "lint:all:js": "pnpm --filter '*' lint:js",
    "lint:js": "eslint . --ignore-pattern test-packages",
    "lint:types": "tsc --noEmit",
    "test": "vitest",
    "update:all": "pnpm --filter '*' update:ignore"
  },
  "dependencies": {
    "@eslint/js": "^9.21.0",
    "@typescript-eslint/parser": "^8.24.1",
    "cosmiconfig": "^9.0.0",
    "ember-eslint": "^0.2.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-decorator-position": "^6.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "globals": "^16.0.0",
    "prettier-plugin-ember-template-tag": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@tsconfig/node-lts": "^22.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "eslint": "9.21.0",
    "lint-to-the-future": "^2.6.2",
    "lint-to-the-future-eslint": "^3.0.0",
    "prettier": "3.5.2",
    "release-plan": "^0.13.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "peerDependencies": {
    "@babel/core": "^7.22.10",
    "eslint": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {},
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/plugin-proposal-decorators": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-ember": {
      "optional": true
    },
    "eslint-plugin-qunit": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">= v22.14.0"
  },
  "volta": {
    "node": "22.14.0"
  }
}
